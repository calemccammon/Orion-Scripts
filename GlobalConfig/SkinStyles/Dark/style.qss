QMainWindow, QDialog, QMessageBox, QFileDialog
{
    border: 1px transparent gray;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 rgb(48, 48, 48), stop:1 rgb(32, 32, 32));
    border-top-color: transparent;
}

QMainWindow::separator, QDialog::separator, QDockWidget::separator
{
    width:6px;
    height:5px;
    padding:2px;
}

QSplitter::handle
{
    background: rgb(128, 128, 128);
}

QSplitter::handle:horizontal
{
    width:4px;
}

QSplitter::handle:vertical
{
    height:4px;
}

QMainWindow::separator:hover, QSplitter::handle:hover
{
    background: rgb(128, 128, 128);
}

QDockWidget
{
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 rgb(48, 48, 48), stop:1 rgb(32, 32, 32));
    color: #acacac;
}

QDockWidget::title
{
    padding:4px;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #6c6c6c, stop:0.95 #363636 stop:1 #6c6c6c);
    color: #acacac;
    border:1px solid rgba(25,25,25,75);
    border-bottom:2px solid rgba(25,25,25,75);
}

QComboBox
{
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #6c6c6c, stop:0.95 #363636 stop:1 #6c6c6c);
    color: #acacac;
    border-radius: 3px;
    border-top: 1px solid SlateGray;
    border-left: 1px solid SlateGray;
    border-bottom: 1px solid DarkSlateGray;
    border-right: 1px solid DarkSlateGray;
    padding: 3px;
    height: 14px;
}

QComboBox:editable
{
    color: white;
}

QComboBox:hover, QPushButton:pressed
{
    background: solid rgb(96, 96, 96);
    border-radius: 3px;
    border-top: 1px solid SlateGray;
    border-left: 1px solid SlateGray;
    border-bottom: 1px solid DarkSlateGray;
    border-right: 1px solid DarkSlateGray;
    padding: 3px;
    color: white;
    height: 14px;
}

QComboBox:focus
{
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #6c6c6c, stop:0.95 #363636 stop:1 #6c6c6c);
    border: 1px solid rgb(153, 217, 234);
    color: white;
}

QComboBox QAbstractItemView
{
    border: 2px solid white;
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #4c4c4c, stop:0.5 #363636 stop:1 #4c4c4c);
    color: gray;
    selection-background-color: lightgray;
    selection-color: black;
}

QPushButton, QToolButton
{
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #5c5c5c, stop:0.5 #363636 stop:1 #5c5c5c);
    border-radius: 2px;
    padding: 5px;
    color: #acacac;
}

QPushButton:hover, QToolButton:hover
{
    background: solid rgb(96, 96, 96);
    color: white;
}

QPushButton:pressed, QToolButton:pressed
{
    background: solid rgb(64, 64, 64);
    padding-left: 2px;
    padding-top: 2px;
    padding-right: 1px;
    padding-bottom: px;
    color: white;
}

QPushButton:disabled, QToolButton:disabled
{
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #3c3c3c, stop:0.95 #161616 stop:1 #3c3c3c);
    color: rgb(64, 64, 64);
    border-radius: 2px;
    padding: 5px;
}

QGroupBox
{
    background: transparent;
    color: #acacac;
    border: 1px solid rgb(164, 164, 164);
    margin-top: 3ex;
}

QGroupBox::title
{
    subcontrol-origin: margin;
    subcontrol-position: top center;
    padding: 0 3px;
}

QLabel, QCheckBox
{
    background: transparent;
    color: #acacac;
}

QCheckBox:hover
{
