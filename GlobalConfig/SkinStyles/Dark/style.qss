    image:url(:/resource/SkinStyles/Images/icon_checkbox_indeterminate_pressed.png);
}

QCheckBox::indicator:indeterminate:disabled, QListView::indicator:indeterminate:disabled, QTreeView::indicator:indeterminate:disabled, QTableView::indicator:indeterminate:disabled, QGroupBox::indicator:indeterminate:disabled
