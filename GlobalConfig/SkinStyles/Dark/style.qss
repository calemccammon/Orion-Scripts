QMenuBar
{
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #6c6c6c, stop:0.95 #363636 stop:1 #6c6c6c);
    color: #acacac;
    border-bottom:2px solid rgba(25,25,25,75);
}

QMenuBar::item
{
    spacing:2px;
    padding:3px 4px;
    background:transparent;
}

QMenuBar::item:selected
{
    background-color: rgb(192, 192, 192);
    color: black;
    border-left:1px solid rgba(106,106,106,127);
    border-right:1px solid rgba(106,106,106,127);
}

QMenuBar::item:pressed
{
    background-color: rgb(192, 192, 192);
    border-left:1px solid rgba(25,25,25,127);
    border-right:1px solid rgba(25,25,25,127);
}

QMenu
{
    background-color: rgb(64, 64, 64);
    color: #acacac;
    border:1px solid palette(shadow);
}

QMenu::item
{
    padding:3px 25px 3px 25px;
    border:1px solid transparent;
}

QMenu::item:disabled
{
    background-color:rgba(35,35,35,127);
    color:palette(disabled);
}

QMenu::item:selected
{
    background-color: rgb(192, 192, 192);
    color: black;
}

QMenu::icon:checked
{
    border:1px solid palette(highlight);
    border-radius:2px;
}

QMenu::separator
{
    height:1px;
    background:palette(alternate-base);
    margin-left:5px;
    margin-right:5px;
}

QMenu::indicator
{
    width:18px;
    height:18px;
}

QMenu::indicator:non-exclusive:checked
{
    padding-left:2px;
}

QMenu::indicator:non-exclusive:unchecked
{
    padding-left:2px;
}

QMenu::indicator:exclusive:checked
{
    padding-left:2px;
}

QMenu::indicator:exclusive:unchecked
{
    padding-left:2px;
}

QTabWidget::pane
{
    background-color:rgba(66,66,66,50%);
    border-top:1px solid rgba(25,25,25,50%);
}

QTabWidget::tab-bar
{
    left:3px;
    top:1px;
}

QTabBar
{
    background-color:transparent;
    qproperty-drawBase:0;
    border-bottom:1px solid rgba(25,25,25,50%);
    color: #acacac;
}

QTabBar::tab
{
    padding:4px 6px;
    background-color:qlineargradient(x1:0,y1:1,x2:0,y2:0,stop:0 rgba(25,25,25,127),stop:1 rgba(53,53,53,75));
    border:1px solid rgba(25,25,25,75);
    border-top-left-radius:4px;
    border-top-right-radius:4px;
}

QTabBar::tab:selected,QTabBar::tab:hover
{
    background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0 rgba(53,53,53,127),stop:1 rgba(66,66,66,50%));
    border-bottom-color:rgba(66,66,66,75%);
}

QTabBar::tab:selected
{
    border-bottom:2px solid palette(highlight);
}

QTabBar::tab::selected:disabled
{
    border-bottom:2px solid rgb(127,127,127);
}

QTabBar::tab:!selected
{
    margin-top:2px;
}

QListView, QTableView, QTreeView
{
    background: rgb(48, 48, 48);
    alternate-background-color:transparent;
    selection-background-color; red;
    border-top: 0px transparent SlateGray;
    border-left: 0px transparent SlateGray;
    border-bottom: 0px transparent SlateGray;
    border-right: 0px transparent SlateGray;
    border: 1px solid rgb(164, 164, 164);
    border-radius: 3px;
    color: #acacac;
}

QListView::item, QTableView::item, QTreeView::item
{
    background-color: transparent;
    border: 1px transparent gray;
    color: rgb(140, 140, 140);
}

QListView::item:hover, QTableView::item:hover, QTreeView::item:hover
{
    background-color: rgba(107, 115, 135, 255);
    border: 1px transparent gray;
    border-radius: 3px;
    color: white;
}

QListView::item:selected, QTableView::item:selected, QTreeView::item:selected
{
    background-color: rgba(107, 115, 135, 128);
    border: 1px transparent gray;
    border-radius: 3px;
    color: white;
}

QListView::item:focus, QTableView::item:focus, QTreeView::item:focus
{
    border:none;
}

QTreeView QHeaderView::section, QTableView QHeaderView::section
{
    background: rgb(48, 48, 48);
    color: white;
    border-style:none;
    border-bottom:1px solid palette(dark);
    padding-left:5px;
    padding-right:5px;
}

QHeaderView::down-arrow
{
    subcontrol-origin: padding;
    subcontrol-position: center left;
    background: rgb(96, 96, 96);
    image: url(:/resource/SkinStyles/Images/down_arrow.png);
    height: 20px;
    width: 20px;
}

QHeaderView::up-arrow
{
    subcontrol-origin: padding;
    subcontrol-position: center left;
    background: rgb(96, 96, 96);
    image: url(:/resource/SkinStyles/Images/up_arrow.png);
    height: 20px;
    width: 20px;
}

QAbstractSpinBox
{
    padding-right: 15px;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #6c6c6c, stop:0.95 #363636 stop:1 #6c6c6c);
    color: #acacac;
    border: 1px solid rgb(164, 164, 164);
    border-radius: 3px;
    height: 20px;
}

QAbstractSpinBox:focus
{
    padding-right: 15px;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #6c6c6c, stop:0.95 #363636 stop:1 #6c6c6c);
    border: 1px solid rgb(153, 217, 234);
    color: #acacac;
    border-radius: 3px;
    height: 20px;
}

QAbstractSpinBox:disabled
{
    padding-right: 15px;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #3c3c3c, stop:0.95 #161616 stop:1 #3c3c3c);
    border: 1px solid rgb(164, 164, 164);
    color: rgb(64, 64, 64);
    border-radius: 3px;
    height: 20px;
}

QSlider::groove:horizontal
{
    border: 1px solid #999999;
    height: 6px; /* the groove expands to the size of the slider by default. by giving it a height, it has a fixed size */
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #84C1FF, stop:1 #007FFF);
    margin: 8px 0px;
}

QSlider::handle:horizontal
{
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #b4b4b4, stop:1 #8f8f8f);
    border: 1px solid #5c5c5c;
    width: 8px;
    min-height:20px;
    margin: -8px 0px; /* handle is placed by default on the contents rect of the groove. Expand outside the groove */
    border-radius: 3px;
}

QSlider::groove:vertical
{
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #84C1FF, stop:1 #007FFF);
    position: absolute; /* absolutely position 4px from the left and right of the widget. setting margins on the widget should work too... */
    left: 4px; right: 4px;
}

QSlider::handle:vertical
{
    height: 10px;
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #b4b4b4, stop:1 #8f8f8f);
    margin: 0 -4px; /* expand outside the groove */
}

QToolTip
{
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #6c6c6c, stop:0.95 #363636 stop:1 #6c6c6c);
    color: #acacac;
    border:1px solid lightgray;
}
